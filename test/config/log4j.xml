<Configuration>
    <Appenders>
        <Console name="stdout">
            <PatternLayout pattern="%c{1} | %d{HH:mm:ss,SSS} | %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="trace">
            <AppenderRef ref="stdout" />
        </Root>
    </Loggers>
</Configuration>
